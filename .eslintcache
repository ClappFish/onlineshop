) => {\n        getUserData();\n    }, []);\n\n    function Headline(props) {\n        console.log(props.text);\n        return <h1 className=\"headline\">{props.text}</h1>;\n    }\n\n    function getUserData() {\n        axios.get(`http://${data_api_uri}:${data_api_port}/getAccountName/${userId}`).then((response) => {\n            setUserName(response.data);\n        });\n        axios.get(`http://${data_api_uri}:${data_api_port}/getAccountMail/${userId}`).then((response) => {\n            setEMail(response.data);\n        });\n    }\n\n    function logOut() {\n        removeCookie(\"userId\", {path: '/'})\n        window.location.href = \"/\";\n    }\n\n    return (\n        <div>\n            <div>\n                <Header/>\n            </div>\n            <br/>\n            <br/>\n            <br/>\n            <Headline\n                text=\"Account\"\n            />\n            <div className=\"useroverview\">\n                <div className=\"userdatas\">\n                    <h3 className=\"userdata\">Username: {userName}</h3>\n                    <h3 className=\"userdata\">E-Mail: {eMail}</h3>\n                </div>\n                <button className=\"logoutButton\" onClick={logOut}>Logout</button>\n            </div>\n        </div>\n    )\n}\nexport default Profile;","/Users/fynnfischbach/Programmiert-Büro/Onlineshop/online-shop/src/Components/UserPages/ProductPage/ProductPage.js",[],"/Users/fynnfischbach/Programmiert-Büro/Onlineshop/online-shop/src/Components/UserPages/Cart/Cart.js",["80"],"import React, {useEffect} from \"react\";\nimport axios from \"axios\";\nimport \"./Cart.css\";\nimport {getUserId} from \"../../../UserIdHandler\";\nimport Header from \"../Navbar/Header\";\nimport TestImage from \"../../Pictures/Christmas-cactus-1-580x386.jpg\";\nimport * as constUrl from \"../../../UrlHandler\";\nimport {MdRemoveShoppingCart} from \"react-icons/md\";\nimport {IoHeartOutline} from \"react-icons/io5\";\nimport checkout from \"../../Pictures/C